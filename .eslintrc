{
	"extends": [
		"wikimedia"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true
		}
	},
	"rules": {
		"no-useless-rename": ["error", {
			"ignoreDestructuring": false,
			"ignoreImport": false,
			"ignoreExport": true
		}],
		"strict": ["error", "global"],
		"one-var": ["error", "never"],
		"space-in-parens": ["error", "never"],
		"no-console": [0],
		"no-undef": [0],
		"array-bracket-spacing": [0],
		"no-unused-vars": [1],
		"computed-property-spacing": [0],
		"camelcase": [1],
		"eqeqeq": [1],
		"no-underscore-dangle": [0],
		"object-curly-spacing": [0]
	},
	"parser": "babel-eslint"
}
